<?xml version="1.0" ?>
<diagram author="Glenn De Jonghe" name="StateChartEditor">
    <description>
        SCCD visual editor for Unity.
    </description>
    
    <top>
        using UnityEditor;
        using UnityEngine;
        using SCCDEditor;
        using System.Xml.Linq;
        using System.Linq;
        using Event = sccdlib.Event;
        #pragma warning disable 0219 // variable assigned but not used.
        #pragma warning disable 0414 // private field assigned but not used.
    </top>
    
    <inport name="input" />
    
    <class name="Window" default="true" src="xml/statechart/window.xml"/>
    <class name="Toolbar" src="xml/statechart/toolbar.xml"/>
    <class name="PropertiesPanel" src="xml/statechart/properties_panel.xml"/>
    <class name="StateProperties" src="xml/statechart/state_properties.xml"/>
    <class name="Action" src="xml/statechart/action.xml"/>
    <class name="Script" src="xml/statechart/script.xml"/>
    <class name="Raise" src="xml/statechart/raise.xml"/>

    <class name="Canvas" src="xml/statechart/canvas.xml"/>
    <class name="State" src="xml/statechart/state.xml"/>
    <class name="StateDrop" src="xml/statechart/state_drop.xml"/>
    <class name="Edge" src="xml/common/edge.xml"/>

</diagram>
