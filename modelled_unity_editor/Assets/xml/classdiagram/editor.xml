<?xml version="1.0" ?>
<diagram author="Glenn De Jonghe" name="ClassDiagramEditor">
    <description>
        SCCD visual editor for Unity.
    </description>
    
    <top>
        using UnityEditor;
        using UnityEngine;
        using SCCDEditor;
        using System.Xml.Linq;
        using System.Linq;
        using Event = sccdlib.Event;
        using csharp_sccd_compiler;
        using Association = sccdlib.Association;
        #pragma warning disable 0219 // variable assigned but not used.
        #pragma warning disable 0414 // private field assigned but not used.
    </top>
    
    <inport name="input" />
    
    <class name="Window" default="true" src="window.xml"/>
    <class name="MenuStrip" src="menu_strip.xml"/>
    <class name="Toolbar" src="toolbar.xml"/>
    <class name="PropertiesPanel" src="properties_panel.xml"/>
    <class name="DiagramProperties" src="diagram_properties.xml"/>
    <class name="Port" src="port.xml"/>
    <class name="ClassProperties" src="class_properties.xml"/>
    <class name="Attribute" src="attribute.xml"/>
    <class name="Method" src="method.xml"/>
    <class name="FormalParameter" src="formal_parameter.xml"/>
    <class name="Canvas" src="canvas.xml"/>
    <class name="Class" src="class.xml"/>
    <class name="Relation" src="relation.xml"/>
    <class name="RelationProperties" src="relation_properties.xml"/>
</diagram>
